{
  "name": "@trussworks/react-uswds",
  "version": "7.0.0",
  "description": "React USWDS 3.0 component library",
  "keywords": [
    "react",
    "uswds",
    "uswds 3.0",
    "react component",
    "us web design",
    "design system",
    "component library"
  ],
  "main": "lib/index.js",
